    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "envify": "^4.0.0",
    "jsdom": "~9.11.0",
    "json-loader": "^0.5.0",
    "jsx-loader": "^0.13.2",
    "jsxhint": "^0.15.1",
